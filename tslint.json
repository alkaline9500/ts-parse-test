{
  "extends": "tslint:recommended",
  "rules": {
    "align": false,
    "arrow-parens": false,
    "max-classes-per-file": false,
    "member-access": false,
    "member-ordering": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "triple-equals": false,
    "arrow-return-shorthand": true,
    "prefer-const": true,
    "semicolon": [true, "always"],
    "switch-default": false
  }
}
